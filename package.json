{
  "name": "clif-consortium-website",
  "version": "1.0.0",
  "type": "module",
  "description": "CLIF Consortium website - A healthcare consortium website showcasing the Common Longitudinal ICU data Format standard for critical care data research",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "lint": "eslint . --ext .js,.ts,.astro",
    "lint:fix": "eslint . --ext .js,.ts,.astro --fix",
    "format": "prettier --write \"**/*.{js,ts,astro,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,astro,json,css,md}\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "healthcare",
    "icu",
    "data-standards",
    "critical-care",
    "consortium",
    "astro",
    "typescript"
  ],
  "author": "CLIF Consortium",
  "license": "ISC",
  "dependencies": {
    "@astrojs/sitemap": "^3.4.1",
    "@astrojs/tailwind": "^6.0.2",
    "@sentry/astro": "^9.35.0",
    "astro": "^4.16.18",
    "astro-compress": "^2.3.8",
    "sharp": "^0.32.6",
    "tailwindcss": "^3.4.17",
    "web-vitals": "^5.0.3"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/ts-plugin": "^1.10.4",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-prettier": "^5.5.1",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "typescript": "^5.8.3"
  }
}
